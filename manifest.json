{
  "name": "redirect to mobile wikipedia",
  "version": "0.1",
  "manifest_version": 2,
  "description": "redirect to mobile wikipedia",
  "permissions": [
    "webRequest",
    "*://ja.wikipedia.org/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://ja.wikipedia.org/*"],
      "js": ["myscript.js"]
    }
  ]
}
